# A sample of ~/.w3m/keymap (default)
#
# Ctrl  : C-, ^
# Escape: ESC-, M-, ^[
# Space : SPC, ' '
# Tab   : TAB, ^i, ^I
# Delete: DEL, ^?
# Up    : UP, ^[[A
# Down  : DOWN, ^[[B
# Right : RIGHT, ^[[C
# Left  : LEFT, ^[[D
# Page Down : ^[[6~
# Page Up   : ^[[5~

keymap C-d NULL
keymap C-b NULL
# keymap H NULL
keymap ESC-a NULL
keymap ESC-b NULL
keymap v NULL
keymap a NULL
keymap t NULL
keymap C-q NULL
keymap LEFT NULL

keymap LEFT PREV
keymap RIGHT GOTO_LINK
keymap C-w CLOSE_TAB
keymap C-t NEW_TAB
keymap C-f SUBMIT
keymap C-d ADD_BOOKMARK
keymap C-b BOOKMARK
keymap b SEARCH_PREV

keymap C-RIGHT TAB_LINK

#keymap NEXT_TAB C-^[[6~
#keymap PREV_TAB C-^[[5~
